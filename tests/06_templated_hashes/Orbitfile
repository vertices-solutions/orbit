[blueprint]
name = "06_templated_hashes"

[sync]
include = []
exclude = ["/.git/"]

[template.fields.file_count]
type = "integer"
default = 10
description = "Number of random files to generate and hash."
values = []

[template.fields.hash_method]
type = "enum"
description = "Hashing method for generated files."
values = ["sha256", "sha1", "md5"]

[template.files]
paths = ["run.sh", "inputs.txt"]

[template.presets.md5_100]
file_count = 100
hash_method = "md5"

[template.presets.sha256_20]
file_count = 20
hash_method = "sha256"
